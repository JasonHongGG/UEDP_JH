{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "UEDP",
  "version": "0.1.0",
  "identifier": "com.jasonhong.uedp",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "UEDP",
        "label": "main",
        "width": 750,
        "height": 680,
        "decorations": false,
        "transparent": true
      },
      {
        "title": "Target Process Search",
        "label": "process-selector",
        "width": 600,
        "height": 450,
        "decorations": false,
        "transparent": true,
        "visible": false,
        "center": true,
        "resizable": false,
        "alwaysOnTop": true
      },
      {
        "title": "UE Dumper Console",
        "label": "object-analysis",
        "width": 1280,
        "height": 800,
        "decorations": false,
        "transparent": true,
        "visible": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}